{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "endOfLine": "auto"
    }
}
